
function [] = plotRoofline(B, G)

X = [1/32 1/16 1/8 1/4 1/2 1 2 4 8 16];
Y = B * X;
Z = G * ones(size(X)); 


G 



plot(log2(X), log2(Y),'r-o', 'LineWidth', 5); 
hold on; 
plot(log2(X), log2(Z),'b-o', 'LineWidth', 5); 
hold off; 
xt = get(gca, 'XTick');
yt = get(gca, 'YTick');
set (gca, 'YTickLabel', 2.^yt);
set (gca, 'XTickLabel', 2.^xt);
grid on
end